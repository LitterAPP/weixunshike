page{
  background: #F2F2F2;
}

.priceinfo {
  display: flex;
  flex-direction: row;
  align-items: center;
  align-content: center;
  justify-content: space-between;
  padding: 5px 10px;
  background: #fff;
}

.priceinfo-2 {
  display: flex;
  flex-direction: column;
  align-items: center;
  align-content: center;
}

.share {
  position: relative;
  width: 25px;
  height: 25px;
}

.share image {
  width: 25px;
  height: 25px;
  position: absolute;
  top: 0px;
  left: 0px;
}

.share button {
  opacity: 0;
  height: 30px;
  width: 30px;
  position: absolute;
  top: 0px;
  left: 0px;
}

.priceinfo-2 text {
  color: #666;
  font-size: 12px;
}

.priceinfo-1-2 text {
  color: #666;
  font-size: 14px;
}

.priceinfo-1-1-1 {
  color: #d70606;
  font-size: 17px;
}

.priceinfo-1-1-2 {
  color: #666;
  font-size: 14px;
  text-decoration: line-through;
}

.title {
  padding: 5px 10px;
  background: #fff;
}

.title text {
  font-size: 16px;
  font-weight: bold;
  color: #000;
  line-height: 25px;
}

.attribute {
  display: flex;
  flex-direction: row;
  align-content: center;
  align-items: center;
  justify-content: space-between;
  padding: 5px 10px;
  background: #fff;
  margin-top: 1px;
  height: 30px;
  margin-bottom:10px; 
}

.attribute image {
  height: 15px;
  width: 15px;
}

.attribute text {
  color: #666;
  font-size: 14px;
  padding-left: 5px;
}

.attribute-info {
  display: flex;
  flex-direction: row;
  align-content: center;
  align-items: center;
  justify-content: space-between;
}

.together-list-item-1 {
  display: flex;
  flex-direction: column;
  align-content: center;
  text-align: center;
}

.together-list-item-1 image {
  width: 40px;
  height: 40px;
  border-radius: 50%;
  align-self: center;
}

.together-list-item-1 text {
  color: #668;
  font-size: 13px;
  text-overflow: ellipsis;
  overflow: hidden;
  width: 50px;
  white-space: nowrap;
}

.together-list-title {
  display: flex;
  flex-direction: row;
  align-content: center;
  align-items: center;
  justify-content: space-between;
  padding: 5px 0px;
}

.together-list-title-1 {
  font-size: 15px;
  color: #666;
}

.together-list-title-2 {
  font-size: 13px;
  color: #b4b4b4;
}

.together-list {
  background: #fff;
  margin-top: 10px;
  padding: 5px 10px;
}

.together-list-item {
  display: flex;
  flex-direction: row;
  align-content: center;
  align-items: center;
  justify-content: space-between;
  padding: 5px 0px;
  border-top: 1px solid #F2F2F2;
}

.together-list-item-2 {
  display: flex;
  flex-direction: row;
  align-content: center;
  align-items: center;
  justify-content: space-between;
}

.together-list-item-2-1 {
  display: flex;
  flex-direction: column;
}

.together-list-item-2-1-2 {
  color: #666;
  font-size: 13px;
}

.together-list-item-2-1-1 {
  color: #000;
  font-size: 15px;
}

.together-list-item-3 {
  color: #fff;
  background: #d70606;
  font-size: 15px;
  padding: 5px 10px;
}

.product-desc {
  display: flex;
  flex-direction: column;
  margin-top: 10px;
  background: #fff;
  padding: 5px 0px;
}

.product-desc-1 {
  border-bottom: 1px solid #F2F2F2;
  padding: 5px 10px;
  color: #666;
  font-size: 13px;
}

.product-desc-2 {
  padding: 5px 10px;
  color: #000;
  font-size: 14px;
}

