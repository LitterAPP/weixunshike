.topTips{
  display: flex;
  flex-direction: column;
  align-content: center;
  align-items: center;
  text-align: center;
  position: fixed;
  bottom: 60px;
  right: 10px;
  background: #fff;
  border-radius: 50%;
  padding: 5px;
}
.topTips image{
  width: 30px;
  height: 30px;
}
.topTips text{
  font-size: 12px;
  color: #666;
}


.product-group{
  background: rgba(0, 0, 0, 0.8);
  position: fixed;
  top: 0px;
  left: 0px;
  z-index:1000;
}

.product-group-info{
  display: flex;
  flex-direction: column;
  position: fixed;
  bottom: 0px;
  left: 0px;
  background: #fff;
  align-content: center;
  align-items: center;  
}
.product-group-item{
  padding: 10px;
}
.product-group-item text{
 
}

.group-unselecte{
  font-size: 15px;
  color: #666;
  padding: 5px;
  border-radius: 10px;
  height: 30px;
  line-height: 30px;
}
.group-selected{
  font-size: 15px;
  color: #fff;
  background: #d70606; 
  padding: 5px;
  border-radius: 10px;
  height: 30px;
  line-height: 30px;
}
.product-group-bottom{
  width: 100%;
  height: 60px;
  line-height: 60px;
  background: #d70606;
  color: #fff;
  text-align: center;
  font-size: 18px;
}

.product-group-top{
  position: relative;
  height: 50px;
  border-bottom:1px solid #F2F2F2;
  padding-bottom:5px; 
}

.product-group-top-image{
  height: 80px;
  width: 80px;
  border: 1px solid #b4b4b4;
  border-radius: 50%;
  position: absolute;
  left: 10px;
  top: -30px;
}
.delete-image{
  height:25px;
  width:25px;
  position: absolute;
  right: 5px;
  top: 5px;
}

.product-group-top-1{
  position: absolute;
  left: 100px;
  top: 5px;
  color: #d70606;
  font-size: 15px;
}
.product-group-top-2{
  position: absolute;
  left: 100px;
  top: 25px;
  color: #000;
  font-size: 14px;
  text-overflow:  ellipsis;
  overflow:hidden;   
  white-space:nowrap; 
  margin-right: 5px;
}
.product-group-number{
  display: flex;
  flex-direction: row;
  align-content: center;
  align-items: center;
  justify-content: space-between;
  margin: 5px 10px;
}
.product-group-number text{
  font-size: 14px;
  color: #666;
}


.community{
  height: 10px;
  line-height: 10px; 
  border-radius: 5px;
  background: orange;
  color: #fff;
  padding: 4px;
  text-align: center;
  font-size: 12px;
  margin-left:5px; 
}
.pick{
  height: 10px;
  line-height: 10px;
  border-radius: 5px;
  background: green;
  color: #fff;
  padding: 4px;
  text-align: center;
  font-size: 12px;
  margin-left:5px;
}

.scrollH{
  width: 100%;
  white-space: nowrap;
}

.coupon-container{
  display: inline-flex;
}
